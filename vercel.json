{
  "version": 2,
  "name": "universal-api-proxy",
  "public": true,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.4.1",
      "maxDuration": 10
    }
  }
}
